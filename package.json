{
  "name": "@dativa-lv/lx-builders",
  "description": "LX/UI Builders",
  "prettier": "@zzdats/prettier-config",
  "files": [
    "dist",
    "dist/vite",
    "dist/styles"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dativa-lv/lx-builders/issues"
  },
  "main": "./dist/wntr-lx-ui.umd.js",
  "typings": "./dist/wntr-lx-ui.d.ts",
  "module": "./dist/wntr-lx-ui.esm.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/wntr-lx-ui.esm.js",
      "require": "./dist/wntr-lx-ui.umd.js",
      "types": "./dist/wntr-lx-ui.d.ts"
    },
    "./vite": {
      "import": "./dist/vite/wntr-lx-ui-vite.es.js",
      "types": "./dist/vite/types/index.d.ts"
    },
    "./dist/styles/*": "./dist/styles/*"
  },
  "packageManager": "pnpm@10.7.0",
  "version": "0.0.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host",
    "build:lib": "pnpm vite build --mode lib",
    "build:css": "pnpm vite build --mode css",
    "build": "pnpm build:lib && pnpm build:css",
    "preview": "pnpm vite preview --mode lib",
    "lint": "eslint .",
    "test": "pnpm vitest --mode lib"
  },
  "dependencies": {
    "@vue/test-utils": "^2.4.3",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^13.4.0",
    "@vueuse/integrations": "^11.0.3"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-airbnb": "^7.0.1",
    "@zzdats/prettier-config": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-vue": "^9.20.1",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "happy-dom": "^20.0.2",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.14.1",
    "vite": "5.0.13",
    "vite-plugin-dts": "3.7.1",
    "vitest": "^2.0.4",
    "vue": "^3.5.17"
  },
  "overrides": {
    "brace-expansion": "2.0.2"
  },
  "pnpm": {
    "overrides": {
      "cross-spawn": "7.0.6"
    }
  },
  "peerDependencies": {
    "vue": "^3.5.17",
    "@dativa-lv/lx-ui": "2.0.0-beta"
  }
}
